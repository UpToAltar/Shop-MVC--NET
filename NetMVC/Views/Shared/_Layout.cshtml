@using Microsoft.EntityFrameworkCore
@inject AppDbContext _context
@{
	var menuModel = await _context.Categories.Where(c=> c.IsActive).OrderBy(c => c.Position).ToListAsync();
	var subModel = new Subscriber();
}
<!DOCTYPE html>
<html lang="en">
<head>
<title>Colo Shop</title>
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="description" content="Colo Shop Template">
<meta name="viewport" content="width=device-width, initial-scale=1">
<link href="~/Assets/Admin/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
<link rel="stylesheet" type="text/css" href="~/Assets/Main/styles/bootstrap4/bootstrap.min.css">
<link href="~/Assets/Main/plugins/font-awesome-4.7.0/css/font-awesome.min.css" rel="stylesheet" type="text/css">
<link rel="stylesheet" type="text/css" href="~/Assets/Main/plugins/OwlCarousel2-2.2.1/owl.carousel.css">
<link rel="stylesheet" type="text/css" href="~/Assets/Main/plugins/OwlCarousel2-2.2.1/owl.theme.default.css">
<link rel="stylesheet" type="text/css" href="~/Assets/Main/plugins/OwlCarousel2-2.2.1/animate.css">
<link rel="stylesheet" type="text/css" href="~/Assets/Main/plugins/jquery-ui-1.12.1.custom/jquery-ui.css">
<link rel="stylesheet" type="text/css" href="~/css/pageList.css">
<link rel="stylesheet" type="text/css" href="~/css/loader.css">
<link rel="stylesheet" type="text/css" href="~/css/toast.css">
</head>

<body>

<div class="super_container">

	<partial name="~/Views/Main/_MenuTop.cshtml" model="menuModel" />
    
    <div class="fs_menu_overlay"></div>
    <partial name="~/Views/Main/_MenuHambuger.cshtml" model="menuModel" />
	@RenderBody()
	
	<partial name="~/Views/Main/_Footer.cshtml" model="subModel"/>
</div>

<script src="~/Assets/Main/js/jquery-3.2.1.min.js"></script>
<script src="~/Assets/Main/styles/bootstrap4/popper.js"></script>
<script src="~/Assets/Main/styles/bootstrap4/bootstrap.min.js"></script>
<script src="~/Assets/Main/plugins/Isotope/isotope.pkgd.min.js"></script>
<script src="~/Assets/Main/plugins/OwlCarousel2-2.2.1/owl.carousel.js"></script>
<script src="~/Assets/Main/plugins/easing/easing.js"></script>
<script src="~/Assets/Main/plugins/jquery-ui-1.12.1.custom/jquery-ui.js"></script>
<script src="https://maps.googleapis.com/maps/api/js?v=3.exp&key=AIzaSyCIwF204lFZg1y4kPSIhKaHEXMLYxxuMhA"></script>
<script src="~/Assets/Main/js/categories_custom.js"></script>
<script src="~/Assets/Main/js/contact_custom.js"></script>
<script src="~/Assets/Main/js/single_custom.js"></script>
<script src="~/Assets/Main/js/custom.js"></script>
<script src="~/Assets/JsFunction/ShoppingCart.js"></script>


</body>
@await RenderSectionAsync("scripts", required: false)
</html>
